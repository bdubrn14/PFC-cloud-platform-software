%>
  <form action="/configure" method="post">
    <div class="form-group text-center">
        <table class="table">
          <tr> 
            <th> Variables </th>
            <th> Control Sets </th>
          </tr> 
          <tr> 
            <td>
              <select class="form-control" name="var1">
                <option <%= var1Op1 %> > <%= varOp1 %> </option>
                <option <%= var1Op2 %> > <%= varOp2 %> </option>
                <option <%= var1Op3 %> > <%= varOp3 %> </option>
                <option <%= var1Op4 %> > <%= varOp4 %> </option>
                <option <%= var1Op5 %> > <%= varOp5 %> </option>
                <option <%= var1Op6 %> > <%= varOp6 %> </option>
                <option <%= var1Op7 %> > <%= varOp7 %> </option>
              </select>
            </td>
            <td>
              <select class="form-control" name="sched1">
                <option <%= sched1Op1 %> > <%= schedOp1 %> </option>
                <option <%= sched1Op2 %> > <%= schedOp2 %> </option>
                <option <%= sched1Op3 %> > <%= schedOp3 %> </option>
                <option <%= sched1Op4 %> > <%= schedOp4 %> </option>
              </select>
            </td>
          </tr> 
          <tr> 
            <td>
              <select class="form-control" name="var2">
