                <option <%= var2Op5 %> > <%= varOp5 %> </option>
                <option <%= var2Op6 %> > <%= varOp6 %> </option>
                <option <%= var2Op7 %> > <%= varOp7 %> </option>
              </select>
            </td>
            <td>
              <select class="form-control" name="sched2">
                <option <%= sched2Op1 %> > <%= schedOp1 %> </option>
                <option <%= sched2Op2 %> > <%= schedOp2 %> </option>
                <option <%= sched2Op3 %> > <%= schedOp3 %> </option>
                <option <%= sched2Op4 %> > <%= schedOp4 %> </option>
              </select>
            </td>
          </tr> 
        </table>

        <button type="submit" class="btn btn-primary btn-lg">
          Save Configuration</button>

    </div>
  </form>

  <% if( "undefined" != typeof( info ) && info.length > 0 ) { %>
    <div class="alert alert-info text-center"><%= info %></div>
  <% } %>

  <% if( "undefined" != typeof( warning ) && warning.length > 0 ) { %>
    <div class="alert alert-warning text-center"><%= warning %></div>
  <% } %>


